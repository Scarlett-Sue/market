{
  "name": "shoping",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "buildModern": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --copy",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.vue\"",
    "review": "serve -s dist",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.5",
    "crypto": "^1.0.1",
    "echarts": "^4.2.0-rc.1",
    "element-ui": "^2.8.2",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "ramda": "^0.26.1",
    "script-loader": "^0.7.2",
    "vue": "^2.6.10",
    "vue-i18n": "^8.10.0",
    "vue-pdf": "^4.0.7",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.3",
    "vue2-verify": "^1.1.5",
    "vuex": "^3.0.1",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-mocha": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.11.0",
    "postcss-px-to-viewport": "1.1.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.0.0",
    "style-resources-loader": "1.2.1",
    "svg-sprite-loader": "^4.1.6",
    "vue-markdown": "^2.2.4",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "lint-staged": {
    "**/**.{js,json,pcss,md,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
